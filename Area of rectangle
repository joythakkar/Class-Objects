#include<iostream>
using namespace std;
 class Rectangle{
        public:
        float width;
        float length;
        void area(){
           float area=length*width;
           cout<<"Area of rectange is:"<<area<<endl;
} 
    }; 
 


int main(){
   

Rectangle R1;
R1.length=0;
R1.width=0;
cout<<"Length of rectangle is:"<<endl;
cin>>R1.length;
cout<<"Width of rectangle is:"<<endl;
cin>>R1.width; 
 
if(R1.length==R1.width){
    cout<<"It is a square."<<endl;
}else{
   R1.area();
   
}

    return 0;
}
